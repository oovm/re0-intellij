/// 所有自定义属性的加和
属性 属性 {
    求和 [智力]
}

/// 重开次数
属性 重开 {

}

属性 分数 {
    别称 [分, SCORE]
}

属性 世界 {
    枚举 [
        /// 现代的地球
        地球
    ]
}

属性 种族 {
    别称 [族, RACE]
    枚举 [
        人
    ]
}

属性 性别 {
    别称 [SEX]
    枚举: [
        男
        女
    ]
}


// 预期寿命, 超过后会增加死亡率
属性 寿命 {
    别称 [寿, DETAH]
}

// 年龄, 每年 + 1
属性 年龄 {
    别称 [龄, AGE]
}

属性 体质 {
    别称 [体, STR]
}

属性 智力 {
    别称 [智, INT]
}

属性 财富 {
    别称 [财, WAM]
}