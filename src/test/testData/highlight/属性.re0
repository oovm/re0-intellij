/// 所有自定义属性的加和
属性 属性 正整数 内置 {

}

/// 重开次数
属性 重开 正整数 内置 {

}

属性 世界 {
    枚举 [
        地球
    ]
}

属性 种族 {
    别称 [族, RACE]
    枚举 [
        人
        妖
    ]
}

属性 性别 {
    别称 [SEX]
    枚举: [
        男
        女
    ]
}


// 预期寿命
属性 寿命 正整数 {
    别称 [寿, AGE]
}

// 年龄, 每年 + 1
属性 年龄 正整数 {
    别称 [龄, AGE]
}

属性 智力 {
    别称 [智, INT]
}